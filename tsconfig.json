{
    "include": ["client/src/**/*", "shared/**/*", "server/**/*", "backend/index.ts", "backend/routes.ts", "backend/server/geminibackend/server/storagebackend/server/vite.tsackend/vite.ts"],
    "exclude": ["node_modules", "build", "dist", "**/*.test.ts"],
    "compilerOptions": {
      "incremental": true,
      "tsBuildInfoFile": "./node_modules/typescript/tsbuildinfo",
      "noEmit": true,
      "module": "ESNext",
      "strict": true,
      "lib": ["esnext", "dom", "dom.iterable"],
      "jsx": "preserve",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "allowImportingTsExtensions": true,
      "moduleResolution": "bundler",
      "baseUrl": ".",
      "types": ["node", "vite/client"],
      "paths": {
        "@/*": ["./client/src/*"],
        "@shared/*": ["./shared/*"]
      }
    }
  }